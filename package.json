{
    "name": "coco",
    "displayName": "Coco",
    "description": "Syntax Highlighting for Cocolang on Visual Studio Code",
    "version": "0.0.1",
    "license": "MIT",
    "publisher": "sarvalabs",
    "author": {
        "name": "Coco Team at Sarva Labs"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/sarvalabs/vscode-coco"
    },
    "bugs": {
        "url": "https://github.com/sarvalabs/vscode-coco/issues"
    },
    "engines": {
        "vscode": "^1.59.0",
        "node": ">=12.0.0"
    },
    "categories": [
        "Plugins",
        "Syntax Highlighting",
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "coco",
                "extensions": [
                    ".coco"
                ],
                "aliases": [
                    "Coco",
                    "Cocolang"
                ],
                "icon": {
                    "light": "./icons/coco.png",
                    "dark": "./icons/coco.png"
                },
                "configuration": "./languages/language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "coco",
                "scopeName": "source.coco",
                "path": "./syntaxes/coco.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "clean": "rm -rf ./dist/* && rm *.vsix",
        "package": "vsce package"
    },
    "devDependencies": {
        "@vscode/vsce": "^2.19.0"
    }
}
